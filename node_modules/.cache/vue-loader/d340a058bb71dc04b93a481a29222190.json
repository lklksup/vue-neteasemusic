\">{{ sml.name }}\n                    </router-link> <i class=\"pointer\" v-if=\"sml.alias.length != 0\"></i>\n                </li>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'other',\n    data() {\n        return {\n            all_artist_list: [],\n            nlist: [],\n            artist1: [],\n            artist2: [],\n            initialNum: '热门',\n        }\n    },\n    computed: {\n        area() {\n            return this.$route.query.area;\n        },\n        type() {\n            return this.$route.query.type;\n        },\n        initial() {\n            return this.$route.query.initial;\n        }\n    },\n    methods: {\n        // 获取热门歌手\n        async getArtistTop(limit) {\n            let result = await this.$API.reqArtists(limit);\n            this.all_artist_list = result.artists;\n        },\n        //获取分类歌手\n        async getArtistsList() {\n            let { area, type, initial } = this;\n            if (initial == undefined) initial = -1;\n            let result = await this.$API.reqArtistsList(type, area, initial, 100, 0);\n            this.artist1 = result.artists.slice(0, 10);\n            this.artist2 = result.artists.slice(10);\n        },\n        //改变initial值\n        changeInitial(item) {\n            if (item == '热门') item = -1;\n            else if (item == '其他') item = 0;\n            this.$router.push({\n                path: '/home/artist',\n                query: {\n                    type: this.type,\n                    area: this.area,\n                    initial: item,\n                }\n            })\n        }\n    },\n    watch: {\n        area() {\n            this.getArtistsList();\n        },\n        type() {\n            this.getArtistsList();\n        },\n        initial() {\n            if (this.initial == -1) this.initialNum = '热门';\n            else if (this.initial == 0) this.initialNum = '其他';\n            else this.initialNum = this.initial;\n 