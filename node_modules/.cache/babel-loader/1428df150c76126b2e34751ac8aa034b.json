rceRoot":"src/views/Home/DjRadio/Category","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"category\">\n        <div class=\"box\">\n            <div class=\"item1\">\n                <li v-for=\"category,index in categoryList\" :key=\"index\">\n                    <a @click=\"chooseCategory(index,category)\" :class=\"{active:isShow==index}\">\n                        <div class=\"icon\" :style=\"`background:url(${category.picWebUrl})`\"></div>\n                        <em>{{category.name}}</em>\n                    </a>\n                </li>\n            </div>\n        </div>\n  </div>\n</template>\n\n<script>\nexport default {\n    name:'category',\n    data() {\n        return {\n            categoryList:[],\n            isShow:-1,\n        }\n    },\n    computed:{\n        // 获取当前页面选中的dj的id\n        id(){\n            return this.$route.query.id;\n        }\n    },\n    watch:{\n        // 如果发生变化 图标也要发生变化\n        id(){\n            if(this.$route.query.id == undefined){\n                this.isShow = -1;  \n            }\n            else{\n                for(let index = 0;index < this.categoryList.length;index++){\n                    if(this.categoryList[index].id == this.$route.query.id){\n                        this.isShow = index;\n                        break;\n                    }\n                }\n            }\n        }\n    },\n    methods:{\n        //获取category数据\n        async getCategory(){\n            let result = await this.$API.reqCateList();\n            this.categoryList = result.categories;\n        },\n        //更换选择目标\n        chooseCategory(index,item){\n            this.isShow = index;\n            this.$router.push({\n                path:`${this.$route.path}`,\n                query:{\n                    id:`${item.id}`,\n                }\n            })\n        }\n    },\n    mounted(){\n        this.getCategory();\n        setTimeout(() => {\n            if(this.$route.query.id != undefined){\n                for(let index = 0;index < this.categoryList.length;index++){\n                    if(this.categoryList[index].id == this.$route.query.id){\n                        this.isShow = index;\n                        break;\n                    }\n                }\n            }    \n        }, 100);\n        \n    }\n}\n</script>\n\n<style lang=\"less\" scoped>\n    .category{\n        width: 100%;\n        overflow: hidden;\n\n        .box{\n            width: 1800px;\n            height: 194px;\n            \n            .item1{\n                float: left;\n                width: 933px;\n                height: 194px;\n                margin-left: -33px;\n\n                li{\n                    display: block;\n                    float: left;\n                    margin-left: 33px;\n                    margin-bottom: 25px;\n                    width: 70px;\n                    height: 72px;\n\n                    a{\n                        display: block;\n                        width: 70px;\n                        height: 70px;\n                        text-align: center;\n                        color: #888;\n                        border-radius: 3px;\n                        &:hover{\n                            background: url(../images/radio_bg.png) no-repeat 0px 0 ;\n                            cursor: pointer;\n                        }\n                        .icon{\n                            width: 48px;\n                            height: 48px;\n                            margin: 2px auto 0;\n                            background-position: 0 0;\n                        }\n                        em{\n                            margin-top: -3px;\n                        }\n                    }\n                    //选中某一个时\n                    .active{\n                         background: url(../images/radio_bg.png) no-repeat -70px 0 ;\n                         &:hover{\n                             background-position: -70px 0; 